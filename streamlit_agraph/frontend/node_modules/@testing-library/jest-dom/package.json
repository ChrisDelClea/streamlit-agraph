{
  "name": "@testing-library/jest-dom",
  "version": "4.2.4",
  "description": "Custom jest matchers to test the state of the DOM",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "scripts": {
    "format": "kcd-scripts format",
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "setup": "npm install && npm run validate -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "files": [
    "dist",
    "extend-expect.js",
    "extend-expect.d.ts"
  ],
  "keywords": [
    "testing",
    "dom",
    "jest",
    "jsdom"
  ],
  "author": "Ernesto Garcia <gnapse@gmail.com> (http://gnapse.github.io/)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.1",
    "chalk": "^2.4.1",
    "css": "^2.2.3",
    "css.escape": "^1.5.1",
    "jest-diff": "^24.0.0",
    "jest-matcher-utils": "^24.0.0",
    "lodash": "^4.17.11",
    "pretty-format": "^24.0.0",
    "redent": "^3.0.0"
  },
  "devDependencies": {
    "jest-watch-select-projects": "^0.1.2",
    "jsdom": "^15.1.0",
    "kcd-scripts": "^1.4.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "babel/no-invalid-this": "off",
      "import/prefer-default-export": "off",
      "import/no-unassigned-import": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/testing-library/jest-dom.git"
  },
  "bugs": {
    "url": "https://github.com/testing-library/jest-dom/issues"
  },
  "homepage": "https://github.com/testing-library/jest-dom#readme"
}
